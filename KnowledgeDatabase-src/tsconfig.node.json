{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.vite.config.*",
    "src/main/**/*",
    "src/preload/**/*",
    "src/utility/**/*",
    "src/Public/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "types": ["electron-vite/node"],
    "baseUrl": ".",
    "paths": {
      "@shared/*": ["src/Public/ShareTypes/*"],
      "@shared-utils": ["src/Public/SharedUtils"],
      "@shared-utils/*": ["src/Public/SharedUtils/*"]
    }
      "@shared-utils/*": ["src/Public/SharedUtils/*"]
    }
  }
}
